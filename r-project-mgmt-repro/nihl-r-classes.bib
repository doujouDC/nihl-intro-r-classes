@article{RN1179,
   author = {Gartlehner, G. and Nussbaumer-Streit, B. and Gaynes, B. N. and Forneris, C. A. and Morgan, L. C. and Greenblatt, A. and Wipplinger, J. and Lux, L. J. and Van Noord, M. G. and Winkler, D.},
   title = {Second-generation antidepressants for preventing seasonal affective disorder in adults},
