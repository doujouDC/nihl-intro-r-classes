@article{RN1245,
   author = {Hampel, C. and Artibani, W. and Pons, M. E. and Haab, F. and Jackson, S. and Romero, J. and Gavart, S. and Papanicolaou, S.},
   title = {Understanding the burden of stress urinary incontinence in Europe: A qualitative review of the literature},
   journal = {European Urology},
   volume = {46},
   number = {1},
   pages = {15-27},
   note = {Hampel, C Artibani, W Pons, ME Haab, F Jackson, S Romero, J Gavart, S Papanicolaou, S
1873-7560},
   abstract = {Objective: This study was undertaken to better define the prevalence, risk factors, as well as the economic and human burden of stress urinary incontinence (SUI) in Europe that will help health care providers better understand the impact of this condition. Methods: A literature search was performed using MEDLINE, EMBASE, BIDS, HealthStar, HSRProj, and HSTAT databases to review the European literature (France, Germany, Italy, Spain, UK) on SUI between 1990 and 2001. Clinical experts on Ul provided advice on this review. Results: In all European countries studied SUI was identified as a distinct and highly prevalent form of urinary incontinence, and in some countries, as many as 80% of all incontinent women were diagnosed with SUI. However, epidemiological figures varied widely between studies, due to different definitions of diagnostic criteria and disease states and study designs used. Parity and vaginal birth were identified as major risk factors for developing SUI. Only a few studies have examined the economic burden of SUI, but have found it to be significant on patients and society. SUI also significantly affects the quality of women's lives. Conclusion: There is a clear need for harmonisation in defining SUI, standardising survey methods and validation criteria, and outcome measures, if results are to be compared with any validity. A better understanding of the burden of SUI is essential for future research. (C) 2004 Elsevier B.V.. All fights reserved.},
   ISSN = {0302-2838},
   DOI = {10.1016/j.eururo.2004.02.003},
   url = {<Go to ISI>://WOS:000222175100003},
   year = {2004},
   type = {Journal Article}
}

@phdthesis{RN1246,
   author = {Boex, James R.},
   title = {Testing the market failure status of graduate medical education},
   university = {The University of Akron},
   note = {Copyright - Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.
Last updated - 2021-01-07},
   type = {Ph.D.},
   abstract = {It is the social scientist's task to employ theory to improve praxis, and to use praxis to challenge theory. This dissertation is an attempt to utilize economic theory to improve the practice of policy making and to employ empirical data to further the debate within economic theory. Neoclassical economic theory allows that the market may not always provide the most efficient means of allocating resources, calling such situations market failures. Public goods are a type of market failure that the market has a disincentive to produce without public subsidies. Graduate medical education has been argued to be such a market failure since, due to physician mobility, teaching hospitals cannot be guaranteed to recoup their costs of training. Service to teaching hospitals is, however, one of the products produced when resident physicians learn their specialties. The level of this service has been unmeasured, and has been assumed not to provide an important incentive to hospitals to undertake graduate medical education. As a result of this, the federal government has provided a special subsidy to teaching hospitals since 1965, and most states have followed suit. This public intervention into the medical workforce marketplace is controversial for a number of reasons, including its effects on physician supply and distribution. This dissertation applies empirical tests—a systematic review of the literature on residents' activities in hospitals, and an activity analysis of primary care residents in ambulatory care settings—to learn what proportion of residents' activities have marginal or no educational value and are undertaken for reasons of providing service. Findings include that 35% of residents' activities are ‘pure’ service, with marginal or no educational value, and that an additional 51% of activities combine service and education. These findings vary by type and year of training. This level of service may provide an incentive to teaching hospitals to offer residency training which is not consistent with market failure status. Since these findings are an initial attempt to measure the content of residents' activities, further empirical research both to confirm and expand these findings, and to overcome other limitations of this study, is appropriate.},
   keywords = {Health and environmental sciences
Social sciences
Graduate education
Market failure
Medical education
Public administration
