@article{RN1177,
   author = {O'Connell, N. E. and Marston, L. and Spencer, S. and DeSouza, L. H. and Wand, B. M.},
   title = {Non-invasive brain stimulation techniques for chronic pain},
   journal = {Cochrane Database Syst Rev},
   volume = {4},
   number = {4},
   pages = {Cd008208},
   note = {1469-493x
O'Connell, Neil E
Marston, Louise
Spencer, Sally
DeSouza, Lorraine H
Wand, Benedict M
Journal Article
