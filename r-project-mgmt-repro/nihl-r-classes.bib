@article{RN1664,
   author = {Spence, Ian and Lewandowsky, Stephan},
   title = {Displaying Proportions and Percentages},
   journal = {Applied Cognitive Psychology},
   volume = {5},
   number = {1},
   pages = {61-77},
   abstract = {Pie and bar charts are commonly used to display percentage or proportional data, but professional data analysis have frowned on the use of the pie chart on the grounds that judgments of area are less accurate than judgments of length. Thus the bar chart has been favored. When the amount of data to be communicated is small, some authorities have advocated the use of properly constructed tables, as another option. The series of experiment reported here suggests that there is little to choose between the pie and the bar chart, with the former enjoying a slight advantage if the required judgment is a complicated one, but that both forms of chart are superior to the table. Thus our results do not support the commonly expressed opinion that pie charts are inferior. An analysis of the nature of the task and a review of the psychophysical literature suggest that the traditional prejudice against the pie chart is misguided.},
   keywords = {COGNITIVE psychology
