@article{RN1205,
   author = {Wilson, S. L. and Gangathimmaiah, V.},
   title = {Does prehospital management by doctors affect outcome in major trauma? A systematic review},
   journal = {Journal of Trauma and Acute Care Surgery},
   volume = {83},
   number = {5},
   pages = {965-974},
   note = {Wilson, Stephanie Laura Gangathimmaiah, Vinay
