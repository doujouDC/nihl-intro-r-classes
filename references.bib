  doi = {10.1146/annurev-vision-091517-034239},
  url = {https://doi.org/10.1146/annurev-vision-091517-034239},
  abstract = {Retinal function has long been studied with psychophysical methods in humans, whereas detailed functional studies of vision have been conducted mostly in animals owing to the invasive nature of physiological approaches. There are exceptions to this generalization, for example, the electroretinogram. This review examines exciting recent advances using in vivo retinal imaging to understand the function of retinal neurons. In some cases, the methods have existed for years and are still being optimized. In others, new methods such as optophysiology are revealing novel patterns of retinal function in animal models that have the potential to change our understanding of the functional capacity of the retina. Together, the advances in retinal imaging mark an important milestone that shifts attention away from anatomy alone and begins to probe the function of healthy and diseased eyes.},
  keywords = {Animals Humans Ophthalmoscopy Optics and Photonics Retina/*diagnostic imaging/*physiology Retinal Neurons/*physiology Tomography,Ocular/physiology adaptive optics calcium indicator functional imaging intrinsic signals neurovascular coupling optical coherence tomography,Optical Coherence Vision}
}

@article{RN1546,
  type = {Journal article},
  title = {A Central Mechanism of Chromatic Contrast},
  author = {Shevell, Steven K. and Wei, Jianping},
  date = {2000},
  journaltitle = {Vision Research},
  volume = {40},
  number = {23},
  pages = {3173--3180},
  issn = {0042-6989},
  doi = {10.1016/S0042-6989(00)00167-X},
  url = {https://www.sciencedirect.com/science/article/pii/S004269890000167X},
  abstract = {The color appearance of a light can be altered by introducing a second, surrounding field. This phenomenon, called chromatic induction, is attenuated by chromatic variation within a remote region outside the surround [Shevell \& Wei (1998). Vision Research, 38, 1561-1566]. We now consider the locus of the neural mechanism mediating the attenuation caused by the remote chromatic contrast. In the first experiment, the magnitude of chromatic variation within the remote region is changed either: (i) in the same eye that views the patch judged in color; or (ii) in only the opposite eye. The measurements are virtually the same in both cases, which implies attenuation of chromatic induction is mediated by a central, binocular mechanism. In the second experiment, the patch with its immediate inducing surround is changed in binocular disparity relative to the remote region with chromatic variation. The patch and surround, seen together in one depth plane, are perceived to be in front of, behind, or in the same plane as the remote region with chromatic variation. Attenuation of chromatic induction is strongest when the patch and surround are in the same depth plane as the remote region. This change of color appearance with disparity is consistent with a central binocular process. Overall, the color-appearance measurements are explained by monocular encoding of chromatic differences at edges, and a central binocular mechanism of chromatic-contrast gain control.},
  keywords = {Chromatic induction Chromatic contrast Gain control Contralateral adaptation}
}

