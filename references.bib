  author = {Poldrack, R. A. and Baker, C. I. and Durnez, J. and Gorgolewski, K. J. and Matthews, P. M. and Munafo, M. R. and Nichols, T. E. and Poline, J. B. and Vul, E. and Yarkoni, T.},
