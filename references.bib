@inproceedings{lacherIncludingCodingQestions2018,
  title = {Including Coding Qestions in Video Qizzes for a Flipped {{CS1}}},
  booktitle = {{{SIGCSE}} 2018 - {{Proceedings}} of the 49th {{ACM Technical Symposium}} on {{Computer Science Education}}},
